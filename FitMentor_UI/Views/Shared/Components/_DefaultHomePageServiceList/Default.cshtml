@model List<FitMentor_UI.Dtos.ServiceDtos.ResultServiceDto>
<section class="locations-1" id="locations">
    <div class="locations py-5">
        <div class="container py-lg-5 py-md-4 py-2">
            <div class="heading text-center mx-auto">
                <h3 class="title-big">Hizmetlerimiz</h3>
            </div>
            <div class="row pt-md-5 pt-4">
                @foreach(var item in Model)
                {
                <div class="col-lg-4 col-md-4 col-sm-12 mt-md-0 mt-4">
                    <a href="property-single.html">
                        <div class="box16">
                                <img class="img-fluid" style="height:200px;width:700px" src="@item.ImageUrl" alt="">
                            <div class="box-content">
                                    <h3 class="name">@item.Name</h3>
                            </div>
                        </div>
                    </a>
                </div>
                    <br />
                }
            </div>
        </div>
    </div>
</section>